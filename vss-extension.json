{
    "manifestVersion": 1,
    "id": "SelectiveCheckoutReborn",
    "name": "Selective Checkout Reborn",
    "version": "0.2.71",
    "publisher": "NicoDGK",
    "galleryFlags": [
        "Public",
        "Preview"
    ],
    "tags": [
        "git",
        "checkout",
        "selective",
        "sparse",
        "folder",
        "subfolder",
        "partial"
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/nicolaiwork/SelectiveCheckout"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "A build task that can be used to only checkout a specific folder from a git repository.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "buildandreleasetask"
        }
    ],
    "contributions": [
        {
            "id": "selective-checkout-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildandreleasetask"
            }
        }
    ]
}